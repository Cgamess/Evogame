    \u043E\u0442 \u0441\u043E\u0431\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0433\u043E
    \u0432\u0435\u0441\u0430 \u043A\u043E\u0441\u0442\u0435\u0439). \u0422\u0430\u043A\u0438\u043C
    \u043E\u0431\u0440\u0430\u0437\u043E\u043C, \u0447\u0435\u043C \u0431\u043E\u043B\u044C\u0448\u0435
    \u043A\u043E\u0441\u0442\u0435\u0439 \u0443 \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0430,
    \u0442\u0435\u043C \u0431\u043E\u043B\u044C\u0448\u0435 \u043C\u044B\u0448\u0446
    \u0435\u043C\u0443 \u043D\u0443\u0436\u043D\u043E, \u0447\u0442\u043E\u0431\u044B
    \u0434\u0432\u0438\u0433\u0430\u0442\u044C \u0438\u0445, \u0438\u043D\u0430\u0447\u0435
    \u0443 \u043C\u044B\u0448\u0446 \u043C\u043E\u0436\u0435\u0442 \u043D\u0435 \u0445\u0432\u0430\u0442\u0438\u0442\u044C
    \u0441\u0438\u043B \u0437\u0430\u0441\u0442\u0430\u0432\u0438\u0442\u044C \u043A\u043E\u0441\u0442\u0438
    \u0434\u0432\u0438\u0433\u0430\u0442\u044C\u0441\u044F. \u0422\u0430\u043A\u0436\u0435
