    \u0441\u0432\u043E\u0438\u043C \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u043E\u043C,
    \u043A\u043E\u0442\u043E\u0440\u044B\u0439 \u043F\u0440\u043E\u0431\u0443\u0435\u0442
    \u0440\u0430\u0437\u043D\u044B\u0435 \u0441\u0442\u0440\u0430\u0442\u0435\u0433\u0438\u0438
    \u0434\u0432\u0438\u0436\u0435\u043D\u0438\u044F \u0438 \u0440\u0430\u0434\u0443\u0435\u0442\u0435\u0441\u044C,
    \u043A\u043E\u0433\u0434\u0430 \u0443 \u043D\u0435\u0433\u043E \u043F\u043E\u043B\u0443\u0447\u0430\u0435\u0442\u0441\u044F
