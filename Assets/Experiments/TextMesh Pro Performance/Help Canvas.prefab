  m_Text: "<b><size=35>\u042D\u0432\u043E\u043B\u044E\u0446\u0438\u044F</size></b>\n\n\u042D\u0442\u043E\u0442
    \u0441\u0438\u043C\u0443\u043B\u044F\u0442\u043E\u0440 \u044D\u0432\u043E\u043B\u044E\u0446\u0438\u0438
    \u043F\u043E\u0437\u0432\u043E\u043B\u044F\u0435\u0442 \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u044C
    \u043D\u0430\u0431\u043E\u0440 \u0431\u0430\u0437\u043E\u0432\u044B\u0445 \u043A\u043E\u043C\u043F\u043E\u043D\u0435\u043D\u0442\u043E\u0432
    \u0438 \u043D\u0435\u0439\u0440\u043E\u043D\u043D\u0443\u044E \u0441\u0435\u0442\u044C
    \u0434\u043B\u044F \u0441\u043E\u0437\u0434\u0430\u043D\u0438\u044F \u0441\u0443\u0449\u0435\u0441\u0442\u0432,
    \u043A\u043E\u0442\u043E\u0440\u044B\u0435, \u043A\u0430\u043A \u043C\u044B \u043D\u0430\u0434\u0435\u0435\u043C\u0441\u044F,
    \u0441\u043C\u043E\u0433\u0443\u0442 \u0432\u044B\u043F\u043E\u043B\u043D\u044F\u0442\u044C
    \u0440\u0430\u0437\u043B\u0438\u0447\u043D\u044B\u0435 \u0437\u0430\u0434\u0430\u0447\u0438,
    \u0442\u0430\u043A\u0438\u0435 \u043A\u0430\u043A \u0431\u0435\u0433, \u043F\u0440\u044B\u0436\u043A\u0438,
    \u043F\u0440\u044B\u0436\u043A\u0438 \u0447\u0435\u0440\u0435\u0437 \u043F\u0440\u0435\u043F\u044F\u0442\u0441\u0442\u0432\u0438\u044F
    \u0438 \u043F\u043E\u0434\u044A\u0435\u043C \u043D\u0430 \u043A\u0440\u0443\u0442\u043E\u0439
    \u0441\u043A\u043B\u043E\u043D.\n\n<b><size=20>\u0421\u0443\u0449\u0435\u0441\u0442\u0432\u0430</size></b>\n\n\u041F\u0440\u0435\u0434\u043B\u0430\u0433\u0430\u0435\u043C\u044B\u0435
    \u0421\u0443\u0449\u0435\u0441\u0442\u0432\u0430 \u0438\u043C\u0435\u044E\u0442
    \u0432 \u0441\u0435\u0431\u0435 \u0442\u0440\u0438 \u0441\u043E\u0441\u0442\u0430\u0432\u043B\u044F\u044E\u0449\u0438\u0435-
    \u043A\u043E\u0441\u0442\u0438, \u043C\u044B\u0448\u0446\u044B \u0438 \u0441\u0443\u0441\u0442\u0430\u0432\u044B,
    \u043E \u0444\u0443\u043D\u043A\u0446\u0438\u044F\u0445 \u043A\u043E\u0442\u043E\u0440\u044B\u0445
    \u043C\u044B \u0440\u0430\u0441\u0441\u043A\u0430\u0436\u0435\u043C \u043D\u0438\u0436\u0435.\n\n<b><size=20>\u0421\u0443\u0441\u0442\u0430\u0432\u044B</size></b>\n\n\u0421\u0443\u0441\u0442\u0430\u0432\u044B
    \u044F\u0432\u043B\u044F\u044E\u0442\u0441\u044F \u0441\u0432\u044F\u0437\u044B\u0432\u0430\u044E\u0449\u0438\u043C
    \u0437\u0432\u0435\u043D\u043E\u043C \u043C\u0435\u0436\u0434\u0443 \u043A\u043E\u0441\u0442\u044F\u043C\u0438.
    \u0411\u0435\u0437 \u043D\u0438\u0445 \u043C\u044B \u043D\u0435 \u0441\u043C\u043E\u0436\u0435\u043C
    \u043F\u043E\u0434\u0441\u043E\u0435\u0434\u0438\u043D\u0438\u0442\u044C \u0434\u0440\u0443\u0433
    \u043A \u0434\u0440\u0443\u0433\u0443 \u043A\u043E\u0441\u0442\u0438. \u0421\u0443\u0441\u0442\u0430\u0432\u044B
    \u043F\u043E\u0437\u0432\u043E\u043B\u044F\u044E\u0442 \u0441\u043E\u0435\u0434\u0438\u043D\u0435\u043D\u043D\u044B\u043C
    \u043A\u043E\u0441\u0442\u044F\u043C \u0434\u0432\u0438\u0433\u0430\u0442\u044C\u0441\u044F
    \u0432\u043E\u043A\u0440\u0443\u0433 \u0446\u0435\u043D\u0442\u0440\u0430 \u0441\u0443\u0441\u0442\u0430\u0432\u0430.
